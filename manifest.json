{
  "manifest_version": 2,
  "name": "Minimalist Calculator",
  "version": "1.0.2",
  "description": "Type your math problem, and copy your answer with one click!",
  "author": "Ian Valeta",
  "offline_enabled": true,
  "icons": {
    "16": "images/icon_16.png",
    "32": "images/icon_32.png",
    "48": "images/icon_48.png",
    "128": "images/icon_128.png"
  },
  "browser_action": {
    "default_icon": {
      "16": "images/icon_16.png",
      "32": "images/icon_32.png"
    },
    "default_popup": "calc.html"
  },
  "permissions": []
}
