{
  "manifest_version": 2,
  "name": "Minimalist Calculator",
  "version": "1.0.2",
  "description": "Type your math problem, and copy your answer with one click!",
  "author": "Ian Valeta",
  "offline_enabled": true,
  "icons": {
    "16": "icon.png",
    "32": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "calc.html"
  },
  "permissions": []
}
